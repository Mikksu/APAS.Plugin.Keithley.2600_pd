<UserControl x:Class="APAS.Plugin.KEYTHLEY._2600_PD.Views.Keithley2600View"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:views="clr-namespace:APAS.Plugin.KEYTHLEY._2600_PD.Views"
             xmlns:local="clr-namespace:APAS.Plugin.KEYTHLEY._2600_PD"
             mc:Ignorable="d" 
             d:Background="White"
             d:DesignHeight="400" d:DesignWidth="500"
             d:DataContext="{d:DesignInstance Type=local:Keithley2600, IsDesignTimeCreatable=False}">

    <Grid>
      <Grid.ColumnDefinitions>
          <ColumnDefinition/>
          <ColumnDefinition/>
      </Grid.ColumnDefinitions>
        <views:SingleChannelControl DataContext="{Binding ChannelA}"/>
        <views:SingleChannelControl Grid.Column="1" DataContext="{Binding ChannelB}"/>
    </Grid>
</UserControl>
